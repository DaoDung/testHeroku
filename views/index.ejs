<!DOCTYPE html>
<html>
<head>
	<title></title>
	<style type="text/css">
		.grid {
		  position: relative;
		}
		.item {
		  display: block;
		  position: absolute;
		  width: 200px;
		  height: 200px;
		  margin: 5px;
		  z-index: 1;
		  text-align: center;
		  line-height: 200px;
		}
		.item1{
		  display: block;
		  position: absolute;
		  width: 400px;
		  height: 400px;
		  margin: 5px;
		  z-index: 1;
		  text-align: center;
		  line-height: 200px;
		}
		.item.muuri-item-dragging {
		  z-index: 3;
		}
		.item.muuri-item-releasing {
		  z-index: 2;
		}
		.item.muuri-item-hidden {
		  z-index: 0;
		}
		.item-content {
		  position: relative;
		  width: 100%;
		  height: 100%;
		  background-color: #96ceb4;
		  text-align: center;
		}
	</style>
</head>
<body>
	<div class="grid">

	  <div class="item">
	    <div class="item-content">
	    	1
	    </div>
	  </div>

	  <div class="item">
	    <div class="item-content">
			2	
	    </div>
	  </div>
	  <div class="item1">
	    <div class="item-content">
			3	
	    </div>
	  </div>
	  <div class="item">
	    <div class="item-content">
			4	
	    </div>
	  </div>
	  <div class="item">
	    <div class="item-content">
			5	
	    </div>
	  </div>
	  <div class="item">
	    <div class="item-content">
			6	
	    </div>
	  </div>
	  <div class="item">
	    <div class="item-content">
			7	
	    </div>
	  </div>
	  <div class="item">
	    <div class="item-content">
			8	
	    </div>
	  </div>
	  <div class="item">
	    <div class="item-content">
			9	
	    </div>
	  </div>

	</div>
	<script src="https://cdnjs.cloudflare.com/ajax/libs/hammer.js/2.0.8/hammer.min.js"></script>
 	<script src="https://cdnjs.cloudflare.com/ajax/libs/web-animations/2.3.1/web-animations.min.js"></script>
	<script src="https://unpkg.com/muuri@0.7.1/dist/muuri.min.js"></script>
	<script type="text/javascript">
		const grid = new Muuri('.grid',{
			dragEnabled: true,
			dragAxis: 'xy',
			layoutOnResize :  true 
		});

	</script>
</body>
</html>